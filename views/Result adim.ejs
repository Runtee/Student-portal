<!DOCTYPE html>
<html lang="en">

<%- include('layouts/head adim'); -%>


<body>

  <%- include('layouts/header adim'); -%>

  <div class="container-fluid">
    <div class="row">
      <%- include('layouts/sidebar adim'); -%>

      <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
        <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3">
          <h1 class="h2">Add result</h1>
        </div>
        <div class="row">
          <form class="col-6 " action="/adim/dashboard/result/store" method="POST">
            <p>
              Input registration number
              <input class="form-control" name="regNo">
            </p>
            <p>Select session <select id="session" class="form-select" name="session">
                <option value="Select session">Select session</option>

                <option value="2021/2022">2021/2022 </option>
                <option value="2020/2021">2020/2021 </option>
                <option value="2019/2020">2019/2020 </option>
                <option value="2018/2017">2018/2017 </option>
                <option value="2027/2018">2027/2018 </option>
                <option value="2016/2017">2016/2017 </option>
                <option value="2015/2016">2015/2016 </option>
                <option value="2014/2015">2014/2015 </option>


              </select></p>

            <p>Select semester <select id="semester" class="form-select" name="semester">
                <option></option>

                <option value="first semester">first semester</option>
                <option value="second semester">second semester</option>

              </select></p>

            <p>Select level <select id="level" class="form-select" name="level">
                <option value="Select level">Select level</option>

                <option value="100">100</option>
                <option value="200">200</option>
                <option value="300">300</option>
                <option value="400">400</option>
                <option value="500">500</option>
                <option value="600">600</option>

              </select>
            </p>
            <button class="btn btn-primary btn-lg">Submit</button>

          </form>
        </div>
      </main>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>

</body>

</html>